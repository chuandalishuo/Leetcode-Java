public class Solution {
    public int maxSubArray(int[] A) {
        if(A.length==0) return 0;
        int[] S = new int[A.length]; S[0]=A[0];
        int maxs = A[0];
        for(int i=1; i<A.length;i++){
            S[i] = (S[i-1]>0)? S[i-1]+A[i] : A[i];//动规
            maxs = Math.max(maxs, S[i]);//贪心
        }
        return maxs;
    }
}
