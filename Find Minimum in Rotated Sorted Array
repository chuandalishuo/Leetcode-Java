public class Solution {
    public int findMin(int[] num) {
        int b=0, e=num.length-1, m=(b+e)/2;
        while(b<e){
            m=(b+e)/2;
            if(num[e]>num[b]) return num[b];
            else{
                if(num[m]==num[b]) return Math.min(num[b],num[e]);
                else if(num[m]>num[b]) b=m;
                else e=m;
            }
        }
        return num[b];
    }
}
